@model IEnumerable<firstWeek.Models.客戶報表Viewmodel> 


@{
    ViewBag.Title = "報表";
}

<h2>@ViewBag.Title</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.客戶名稱)
        </th>
        <th>
            @Html.DisplayNameFor(M=>M.FirstOrDefault().銀行數量)
            
        </th>
        <th>
            @Html.DisplayNameFor(model => model.聯絡人數量)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(m => item.客戶名稱)
        </td>
        <td>
            @Html.DisplayFor(m => item.銀行數量)
        </td>
        <td>
            @Html.DisplayFor(m => item.聯絡人數量)
        </td>

    </tr>
}

</table>
